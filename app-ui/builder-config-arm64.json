{
  "appId": "electron-starter-app.app",
  "productName": "ElectronStarterApp",
  "icon": "../resources/icons/icon.png",
  "copyright": "CopyrightÂ© 2025, Mathew Parker",
  "buildVersion": "1.0.0",
  "compression": "maximum",
  "asar": true,
  "extraResources": [
    "preload.js",
    {
      "from": "./dist/api",
      "to": "api",
      "filter": [
        "**/*"
      ]
    }
  ],
  "files": [
    "app.js",
    {
      "from": "./dist/ui",
      "to": "./",
      "filter": [
        "**/*"
      ]
    },
    {
      "from": "./public",
      "to": "./",
      "filter": [
        "**/*"
      ]
    }
  ],
  "linux": {
    "target": [
      {
        "target": "appImage",
        "arch": [
          "arm64"
        ]
      }
    ],
    "icon": "../resources/icons/",
    "category": "Utility",
    "extraResources": [
    ]
  },
  "win": {
    "target": [
      "nsis"
    ],
    "extraResources": [
    ]
  },
  "mac": {
    "target": [
      {
        "target": "dmg",
        "arch": [
          "arm64"
        ]
      }
    ],
    "icon": "../resources/icons/icon.icns",
    "extraResources": [
    ]
  }
}
